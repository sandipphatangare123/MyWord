<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sandip.myword">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.sandip.applicatiion.MyWord"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme.Base">
        <activity
            android:name=".ActivitySplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityLevelScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivitySettingScreen"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name=".ActivityChangeLanguage"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name=".ActivityChapterScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityPlayBoard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActivityWelcomeScreen"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.sandip.customdialog.DialogSuccess"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name="com.sandip.customdialog.DialogFailed"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Transparent" />
        <activity
            android:name=".ActivityHelp"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Transparent" />

        <service
            android:name="com.sandip.util.BackgroundSoundService"
            android:enabled="true" />

        <activity
            android:name=".ActivityChangeTheme"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Transparent" />
        <activity android:name=".BaseActivty" />
        <activity
            android:name=".ActivityContactUs"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Transparent"></activity>
    </application>

</manifest>